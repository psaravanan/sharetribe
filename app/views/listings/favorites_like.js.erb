<%# binding.pry %>
<% if @favorite.present? %>
	$(".listing_<%= @listing.id %>").removeClass("fa fa-heart-o");
	$(".listing_<%= @listing.id %>").addClass("fa fa-heart");
<% else %>
	$(".listing_<%= @listing.id %>").removeClass("fa fa-heart");
	$(".listing_<%= @listing.id %>").addClass("fa fa-heart-o");
<% end %>