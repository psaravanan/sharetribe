<% wishlist = Wishlist.find_by_id(@wishlist.id)%>
<% if !wishlist.nil? %>
	$(".wish_<%= @listing.id %>").removeClass("fa-flag-o");
	$(".wish_<%= @listing.id %>").addClass("fa-flag");
<% else %>
	$(".wish_<%= @listing.id %>").removeClass("fa-flag");
	$(".wish_<%= @listing.id %>").addClass("fa-flag-o");
<% end %>